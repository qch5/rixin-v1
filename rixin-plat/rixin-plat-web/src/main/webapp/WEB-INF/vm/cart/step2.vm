                $(this).removeClass("active");
            } else {
                $(this).addClass("active");
            }

            var checkVal = "";

            $("[name=WorkSpan]").each(function () {
                if ($(this).hasClass("active")) {
                    var val = $(this).attr("data-val");
                    if (checkVal == "") {
