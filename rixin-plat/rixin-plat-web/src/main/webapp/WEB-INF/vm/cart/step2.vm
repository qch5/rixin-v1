    function checkChildrenJob() {
        var chlidren = $("#smalljob").val();

        if (chlidren == "") {
            alert("请选择您的需求");
            return false;
        }
        return true;
    }


    function showNewFastIndex(num, func, isNext) {
        if (func != undefined && func != null && func != "") {
            var result = func();

            if (!result) {
                return false;
            }
        }

        $(".NewFast_StepBox").hide();
        $(".NewFast_StepBox").each(function () {
            if ($(this).index() == num) {
                $(this).show();
            }
        });

        if (template == "6" && isNext == true) {
            showClock();
        }

    }

    function lastJumpPrev() {
        var boxNum = $("#StepBoxNum").val();
        var num = boxNum - 1;

        showNewFastIndex(num);
    }


    function sumbitOrder() {

        var result = checkForm();
        if (!result) {
            return false;
        }

        var jdata = {
            url: '/Mobile/FastOrder/FlowPath',
            paramData: $("#formbox").serialize(),
            func: function (result) {
                if (result.r) {
                    window.location.href = "/Mobile/Home/NewFocus?oid=" + result.oid + "&type=1";
                } else {
                    alert(result.msg);
                }
            }
        };
        $.submit(jdata);
    }

    function checkForm() {
        var name = $("#FullName").val();
        if (name == "") {
            alert("请填写您的姓名");
            return false;
        }

        var tel = $("#Tel").val();
        if (tel == "") {
            alert("请填写您的手机号码");
            return false;
        } else {
            if (!(/^1[3|4|5|6|7|8|9][0-9]\d{4,8}$/.test(tel))) {
                alert("手机号不正确");
                return false;
            }
        }

        var areap = $("#Area_P").val();
        if (areap == "") {
            alert("请选择服务地区");
            return false;
        }

        var address = $("#Address").val();
        if (address == "") {
            alert("请填写详细街道地址");
            return false;
        }

        var code = $("#vcode").val();
        if (code == "") {
            alert("请填写验证码");
            return false;
        }

        return true;

    }

