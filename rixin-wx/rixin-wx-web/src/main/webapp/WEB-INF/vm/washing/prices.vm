    $(".nav-head li").on('click', function() {
        var index = $(this).index();
        if (index > 3) {
            var n = index - 4;
            $(".nav-head ul").scrollLeft(145 + (n * 100));
        }
        $(this).addClass("selected").siblings().removeClass("selected");
        $(".price-content ul li").eq(index).show().siblings().hide();
    });
</script>
