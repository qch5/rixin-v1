<script>
    $(".invite").on("click", function () {
        $.ajax({
            url: "/Recommend/GetInvitationPacketFirst",
            type: "POST",
            cache: false,
            async: false,
            success: function (r) {
                if (r.state == 1)  //成功
                {
                    window.location.href = '/Recommend/Invite?id=' + r.InvitationPacketsId;
                } else if (r.state == 2)//异常
                {
                    $.weui.toast(r.msg, function () {
                    }, 'weui_icon_toast');
                }
            },
            error: function (xhr) {
                $.weui.toast('发生异常', function () {
                }, 'weui_icon_toast');
            }
        });

    })
    function Loginoff() {
        var formloginoff = document.getElementById('formloginoff')
        formloginoff.submit()
    }
</script>
